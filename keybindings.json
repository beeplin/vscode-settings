  { "key": "ctrl+alt+e", "command": "workbench.action.evenEditorWidths" },
  { "key": "ctrl+alt+t", "command": "workbench.action.toggleEditorWidths" },
  { "key": "ctrl+alt+enter", "command": "workbench.action.toggleMaximizeEditorGroup", "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups" },
  { "key": "ctrl+alt+enter", "command": "workbench.action.toggleMaximizedPanel", "when": "panelFocus" },
  { "key": "ctrl+alt+j", "command": "workbench.action.joinAllGroups" },
  { "key": "ctrl+alt+delete", "command": "workbench.action.reloadWindow" },
